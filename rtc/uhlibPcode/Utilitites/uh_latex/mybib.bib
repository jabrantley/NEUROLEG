@Article{doud2011continuous,
  author   = {Doud, A. J. and Lucas, J. P. and Pisansky, M. T. and He, B.},
  title    = {Continuous three-dimensional control of a virtual helicopter using a motor imagery based brain-computer interface},
  journal  = {PLoS One},
  year     = {2011},
  volume   = {6},
  number   = {10},
  pages    = {e26322},
  note     = {Doud, Alexander J Lucas, John P Pisansky, Marc T He, Bin eng R01 EB006433/EB/NIBIB NIH HHS/ R01 EB006433-03/EB/NIBIB NIH HHS/ R01 EB007920/EB/NIBIB NIH HHS/ R01 EB007920-04/EB/NIBIB NIH HHS/ R01EB006433/EB/NIBIB NIH HHS/ R01EB007920/EB/NIBIB NIH HHS/ Research Support, N.I.H., Extramural Research Support, Non-U.S. Gov't 2011/11/03 06:00 PLoS One. 2011;6(10):e26322. doi: 10.1371/journal.pone.0026322. Epub 2011 Oct 26.},
  abstract = {Brain-computer interfaces (BCIs) allow a user to interact with a computer
	system using thought. However, only recently have devices capable
	of providing sophisticated multi-dimensional control been achieved
	non-invasively. A major goal for non-invasive BCI systems has been
	to provide continuous, intuitive, and accurate control, while retaining
	a high level of user autonomy. By employing electroencephalography
	(EEG) to record and decode sensorimotor rhythms (SMRs) induced from
	motor imaginations, a consistent, user-specific control signal may
	be characterized. Utilizing a novel method of interactive and continuous
	control, we trained three normal subjects to modulate their SMRs
	to achieve three-dimensional movement of a virtual helicopter that
	is fast, accurate, and continuous. In this system, the virtual helicopter's
	forward-backward translation and elevation controls were actuated
	through the modulation of sensorimotor rhythms that were converted
	to forces applied to the virtual helicopter at every simulation time
	step, and the helicopter's angle of left or right rotation was linearly
	mapped, with higher resolution, from sensorimotor rhythms associated
	with other motor imaginations. These different resolutions of control
	allow for interplay between general intent actuation and fine control
	as is seen in the gross and fine movements of the arm and hand. Subjects
	controlled the helicopter with the goal of flying through rings (targets)
	randomly positioned and oriented in a three-dimensional space. The
	subjects flew through rings continuously, acquiring as many as 11
	consecutive rings within a five-minute period. In total, the study
	group successfully acquired over 85% of presented targets. These
	results affirm the effective, three-dimensional control of our motor
	imagery based BCI system, and suggest its potential applications
	in biological navigation, neuroprosthetics, and other applications.},
  doi      = {10.1371/journal.pone.0026322},
  issn     = {1932-6203 (Electronic) 1932-6203 (Linking)},
  keywords = {*Aircraft Arm Brain Electroencephalography/*methods Electromyography Feedback, Sensory Hand Humans Imagery (Psychotherapy)/*methods Movement *User-Computer Interface},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/22046274},
}

@Article{doyon2005reorganization,
  author   = {Doyon, J. and Benali, H.},
  title    = {Reorganization and plasticity in the adult brain during learning of motor skills},
  journal  = {Curr Opin Neurobiol},
  year     = {2005},
  volume   = {15},
  number   = {2},
  pages    = {161-7},
  note     = {Doyon, Julien Benali, Habib eng Research Support, Non-U.S. Gov't Review England 2005/04/16 09:00 Curr Opin Neurobiol. 2005 Apr;15(2):161-7.},
  abstract = {On the basis of brain imaging studies, Doyon and Ungerleider recently
	proposed a model describing the cerebral plasticity that occurs in
	both cortico-striatal and cortico-cerebellar systems of the adult
	brain during learning of new motor skilled behaviors. This theoretical
	framework makes several testable predictions with regards to the
	contribution of these neural systems based on the phase (fast, slow,
	consolidation, automatization, and retention) and nature of the motor
	learning processes (motor sequence versus motor adaptation) acquired
	through repeated practice. There has been recent behavioral, lesion
	and additional neuroimaging studies that have addressed the assumptions
	made in this theory that will help in the revision of this model.},
  doi      = {10.1016/j.conb.2005.03.004},
  issn     = {0959-4388 (Print) 0959-4388 (Linking)},
  keywords = {Adult Animals Cerebellum/growth & development/physiology Cerebral Cortex/growth & development/physiology Humans Learning/*physiology Motor Skills/*physiology Neural Pathways/growth & development/physiology Neuronal Plasticity/*physiology},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/15831397},
}

@Article{dzirasa2011chronic,
  author   = {Dzirasa, K. and Fuentes, R. and Kumar, S. and Potes, J. M. and Nicolelis, M. A.},
  title    = {Chronic in vivo multi-circuit neurophysiological recordings in mice},
  journal  = {J Neurosci Methods},
  year     = {2011},
  volume   = {195},
  number   = {1},
  pages    = {36-46},
  note     = {Dzirasa, Kafui Fuentes, Romulo Kumar, Sunil Potes, Juan M Nicolelis, Miguel A L ENG P50 MH060451/MH/NIMH NIH HHS/ R33 NS049534-05/NS/NINDS NIH HHS/ MH060451-09S1/MH/NIMH NIH HHS/ P50 MH060451-09S1/MH/NIMH NIH HHS/ R33NS049534/NS/NINDS NIH HHS/ R33 NS049534/NS/NINDS NIH HHS/ NS049534-05/NS/NINDS NIH HHS/ P50MH060451-09S1/MH/NIMH NIH HHS/ Research Support, N.I.H., Extramural Research Support, Non-U.S. Gov't Netherlands 2010/12/01 06:00 J Neurosci Methods. 2011 Jan 30;195(1):36-46. doi: 10.1016/j.jneumeth.2010.11.014. Epub 2010 Nov 27.},
  abstract = {While genetically modified mice have become a widely accepted tool
	for modeling the influence of gene function on the manifestation
	of neurological and psychiatric endophenotypes, only modest headway
	has been made in characterizing the functional circuit changes that
	underlie the disruption of complex behavioral processes in various
	models. This challenge partially arises from the fact that even simple
	behaviors require the coordination of many neural circuits vastly
	distributed across multiple brain areas. As such, many independent
	neurophysiological alterations are likely to yield overlapping circuit
	disruptions and ultimately lead to the manifestation of similar behavioral
	deficits. Here we describe the expansion of our neurophysiological
	recording approach in an effort to quantify neurophysiological activity
	across many large scale brain circuits simultaneously in freely behaving
	genetically modified mice. Using this expanded approach we were able
	to isolate up to 70 single neurons and record local field potential
	(LFP) activity simultaneously across 11 brain areas. Moreover, we
	found that these neurophysiological signals remained viable up to
	16 months after implantation. Thus, our approach provides a powerful
	tool that will aid in dissecting the central brain network changes
	that underlie the complex behavioral deficits displayed by various
	genetically modified mice.},
  doi      = {10.1016/j.jneumeth.2010.11.014},
  issn     = {1872-678X (Electronic) 0165-0270 (Linking)},
  keywords = {Action Potentials/physiology Animals Brain/*physiology Electrodes, Implanted Electrophysiology/instrumentation/*methods Mice Microelectrodes Neurons/physiology Neurophysiology/instrumentation/*methods *Signal Processing, Computer-Assisted},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/21115042},
}

