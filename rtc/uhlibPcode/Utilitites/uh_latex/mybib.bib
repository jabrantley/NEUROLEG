@Article{hiremath2015brain,
  author   = {Hiremath, S. V. and Chen, W. and Wang, W. and Foldes, S. and Yang, Y. and Tyler-Kabara, E. C. and Collinger, J. L. and Boninger, M. L.},
  title    = {Brain computer interface learning for systems based on electrocorticography and intracortical microelectrode arrays},
  journal  = {Front Integr Neurosci},
  year     = {2015},
  volume   = {9},
  pages    = {40},
  note     = {Hiremath, Shivayogi V Chen, Weidong Wang, Wei Foldes, Stephen Yang, Ying Tyler-Kabara, Elizabeth C Collinger, Jennifer L Boninger, Michael L eng Review Switzerland 2015/06/27 06:00 Front Integr Neurosci. 2015 Jun 10;9:40. doi: 10.3389/fnint.2015.00040. eCollection 2015.},
  abstract = {A brain-computer interface (BCI) system transforms neural activity
	into control signals for external devices in real time. A BCI user
	needs to learn to generate specific cortical activity patterns to
	control external devices effectively. We call this process BCI learning,
	and it often requires significant effort and time. Therefore, it
	is important to study this process and develop novel and efficient
	approaches to accelerate BCI learning. This article reviews major
	approaches that have been used for BCI learning, including computer-assisted
	learning, co-adaptive learning, operant conditioning, and sensory
	feedback. We focus on BCIs based on electrocorticography and intracortical
	microelectrode arrays for restoring motor function. This article
	also explores the possibility of brain modulation techniques in promoting
	BCI learning, such as electrical cortical stimulation, transcranial
	magnetic stimulation, and optogenetics. Furthermore, as proposed
	by recent BCI studies, we suggest that BCI learning is in many ways
	analogous to motor and cognitive skill learning, and therefore skill
	learning should be a useful metaphor to model BCI learning.},
  doi      = {10.3389/fnint.2015.00040},
  issn     = {1662-5145 (Electronic) 1662-5145 (Linking)},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/26113812},
}

