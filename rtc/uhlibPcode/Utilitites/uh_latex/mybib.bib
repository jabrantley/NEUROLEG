@Article{manto2012consensusa,
  author   = {Manto, M. and Bower, J. M. and Conforto, A. B. and Delgado-Garcia, J. M. and da Guarda, S. N. and Gerwig, M. and Habas, C. and Hagura, N. and Ivry, R. B. and Marien, P. and Molinari, M. and Naito, E. and Nowak, D. A. and Oulad Ben Taib, N. and Pelisson, D. and Tesche, C. D. and Tilikete, C. and Timmann, D.},
  title    = {Consensus paper: roles of the cerebellum in motor control--the diversity of ideas on cerebellar involvement in movement},
  journal  = {Cerebellum},
  year     = {2012},
  volume   = {11},
  number   = {2},
  pages    = {457-87},
  note     = {Manto, Mario Bower, James M Conforto, Adriana Bastos Delgado-Garcia, Jose M da Guarda, Suzete Nascimento Farias Gerwig, Marcus Habas, Christophe Hagura, Nobuhiro Ivry, Richard B Marien, Peter Molinari, Marco Naito, Eiichi Nowak, Dennis A Oulad Ben Taib, Nordeyn Pelisson, Denis Tesche, Claudia D Tilikete, Caroline Timmann, Dagmar eng R01 HD060306/HD/NICHD NIH HHS/ Review 2011/12/14 06:00 Cerebellum. 2012 Jun;11(2):457-87. doi: 10.1007/s12311-011-0331-9.},
  abstract = {Considerable progress has been made in developing models of cerebellar
	function in sensorimotor control, as well as in identifying key problems
	that are the focus of current investigation. In this consensus paper,
	we discuss the literature on the role of the cerebellar circuitry
	in motor control, bringing together a range of different viewpoints.
	The following topics are covered: oculomotor control, classical conditioning
	(evidence in animals and in humans), cerebellar control of motor
	speech, control of grip forces, control of voluntary limb movements,
	timing, sensorimotor synchronization, control of corticomotor excitability,
	control of movement-related sensory data acquisition, cerebro-cerebellar
	interaction in visuokinesthetic perception of hand movement, functional
	neuroimaging studies, and magnetoencephalographic mapping of cortico-cerebellar
	dynamics. While the field has yet to reach a consensus on the precise
	role played by the cerebellum in movement control, the literature
	has witnessed the emergence of broad proposals that address cerebellar
	function at multiple levels of analysis. This paper highlights the
	diversity of current opinion, providing a framework for debate and
	discussion on the role of this quintessential vertebrate structure.},
  doi      = {10.1007/s12311-011-0331-9},
  issn     = {1473-4230 (Electronic) 1473-4222 (Linking)},
  keywords = {Animals Blinking/physiology Cerebellum/*physiology Conditioning, Classical Consensus Eye Movements/physiology Hand Strength/physiology Humans Kinesthesis Magnetic Resonance Imaging Magnetoencephalography Motor Skills/*physiology Movement/*physiology Oculomotor Muscles/physiology Sensation/physiology Speech/physiology},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/22161499},
}

