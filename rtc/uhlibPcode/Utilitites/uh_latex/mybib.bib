@Article{swett2010neural,
  author   = {Swett, B. A. and Contreras-Vidal, J. L. and Birn, R. and Braun, A.},
  title    = {Neural substrates of graphomotor sequence learning: a combined FMRI and kinematic study},
  journal  = {J Neurophysiol},
  year     = {2010},
  volume   = {103},
  number   = {6},
  pages    = {3366-77},
  note     = {Swett, Bruce A Contreras-Vidal, Jose L Birn, Rasmus Braun, Allen eng 2010/04/09 06:00 J Neurophysiol. 2010 Jun;103(6):3366-77. doi: 10.1152/jn.00449.2009. Epub 2010 Apr 7.},
  abstract = {The goal of this study was to characterize the dynamics and functional
	connectivity of brain networks associated with fast (short-term)
	learning of handwriting using functional magnetic resonance imaging.
	Participants (n = 12) performed a graphomotor sequence learning task
	(naive subjects learning to draw simple, 3-stroke Chinese word characters),
	which focused the learning process on the kinematic aspects of sequence
	learning instead of on the production of the line segments. Learning
	of the graphomotor sequence was demonstrated by a progressive improvement
	in movement smoothness as assessed by normalized jerk scores. Examination
	of the patterns of regional neural activity and functional connectivity
	during sequence learning demonstrated that cortical regions, which
	may support visuomotor mapping components of the task, were active
	prior to subcortical areas that may play a role in encoding and refining
	the novel sequences. Importantly, differences in the time course
	of recruitment of basal ganglia and cerebellar networks suggest distinct
	but integrated roles in the encoding and refining of the handwritten
	sequences. This implies multiple kinematic representations of graphomotor
	trajectories may be encoded at various spatiotemporal scales.},
  doi      = {10.1152/jn.00449.2009},
  issn     = {1522-1598 (Electronic) 0022-3077 (Linking)},
  keywords = {Adult Analysis of Variance Biomechanical Phenomena/physiology Brain/*blood supply/*physiology *Brain Mapping Cognition/physiology Female Humans Image Processing, Computer-Assisted/methods *Language Magnetic Resonance Imaging/methods Male Movement/physiology Oxygen/blood Psychomotor Performance/*physiology Serial Learning/*physiology Statistics as Topic Time Factors},
  type     = {Journal Article},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/20375250},
}

